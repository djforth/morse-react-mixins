{
  "name": "morse-react-mixins",
  "version": "1.0.2",
  "description": "Useful react mixins",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "assets": {
    "assets_in": "./src",
    "assets_out": "./lib",
    "karma": {
      "input": "./spec"
    }
  },
  "scripts": {
    "build": "babel src -x '.js'  -d lib",
    "build:watch": "babel  -x '.js' -w --source-maps src -d lib",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "test": "npm run lint && asset-pipeline-karma",
    "test:watch": "karma start --browsers PhantomJS",
    "test:ie9": "karma start --single-run --browsers \"IE9 - Win7\"",
    "test:ie10": "karma start --single-run --browsers \"IE10 - Win7\"",
    "test:mac": "karma start --single-run --browsers Chrome,Firefox,Safari,Opera"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/morse-react-mixins.git"
  },
  "keywords": [
    "Text",
    "and",
    "CSS",
    "mixins",
    "for",
    "react"
  ],
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/morse-react-mixins/issues"
  },
  "homepage": "https://github.com/djforth/morse-react-mixins#readme",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "@djforth/ap_karma": "1.1.9",
    "@djforth/ap_utils": "^2.0.13",
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "classnames": "^2.2.0",
    "eslint": "^2.3.0",
    "eslint-config-google": "^0.4.0",
    "eslint-plugin-react": "^4.2.3",
    "lodash": "~4.8.2",
    "react-addons-test-utils": "~0.14.8",
    "react-jasmine": "~1.0.0",
    "react": "~0.14.8",
    "react-dom": "~0.14.8"
  },
  "peerDependencies": {
    "classnames": "^2.2.0",
    "lodash": "^4.8.2",
    "react": "~0.14.8",
    "react-dom": "~0.14.8"
  }
}
