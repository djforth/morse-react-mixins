{
  "name": "morse-react-mixins",
  "version": "0.0.0",
  "description": "Useful react mixins",
  "main": "index.js",
  "scripts": {
    "build": "babel  -x '.js' --source-maps --stage 0 src -d lib",
    "build:watch": "babel  -x '.js' -w --source-maps --stage 0 src -d lib",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "test": "npm run lint && ./node_modules/.bin/karma start --browsers PhantomJS --single-run",
    "test:watch": "karma start --browsers PhantomJS",
    "test:ie9": "karma start --single-run --browsers \"IE9 - Win7\"",
    "test:ie10": "karma start --single-run --browsers \"IE10 - Win7\"",
    "test:mac": "karma start --single-run --browsers Chrome,Firefox,Safari,Opera"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/morse-react-mixins.git"
  },
  "keywords": [
    "Text",
    "and",
    "CSS",
    "mixins",
    "for",
    "react"
  ],
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/morse-react-mixins/issues"
  },
  "homepage": "https://github.com/djforth/morse-react-mixins#readme",
  "devDependencies": {
    "babel": "^5.8.9",
    "babel-eslint": "^4.0.5",
    "babelify": "^6.1.3",
    "bower": "^1.4.1",
    "browserify": "^11.0.0",
    "eslint": "^0.24.1",
    "eslint-plugin-react": "^3.0.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.3",
    "karma-browserify": "^4.2.1",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.4.2",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-ievms": "0.0.4",
    "karma-jasmine": "^0.3.6",
    "karma-opera-launcher": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-story-reporter": "^0.3.1",
    "phantomjs": "^1.9.17",
    "react-tools": "^0.13.3",
    "rewireify": "^0.2.2",
    "sinon": "^1.15.4",
    "uglifyify": "^3.0.1",
    "watchify": "^3.3.0"
  },
  "dependencies": {
    "classnames": "^2.1.3",
    "lodash": "^3.10.0"
  }
}
