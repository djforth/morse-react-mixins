{
  "name": "morse-react-mixins",
  "version": "1.2.0",
  "description": "Useful react mixins",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "assets": {
    "assets_in": "./src",
    "assets_out": "./lib",
    "karma": {
      "input": "./spec"
    }
  },
  "scripts": {
    "build": "babel src -x '.js'  -d lib",
    "build:watch": "babel  -x '.js' -w --source-maps src -d lib",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "test": "npm run lint && asset-pipeline-karma",
    "test:watch": "asset-pipeline-karma -w",
    "test:mac": "asset-pipeline-karma -b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/morse-react-mixins.git"
  },
  "keywords": [
    "Text",
    "and",
    "CSS",
    "mixins",
    "for",
    "react"
  ],
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/morse-react-mixins/issues"
  },
  "homepage": "https://github.com/djforth/morse-react-mixins#readme",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "@djforth/ap_karma": "1.4.1",
    "@djforth/ap_utils": "^2.1.1",
    "@djforth/react-jasmine": "~1.2.1",
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "classnames": "^2.2.5",
    "eslint": "^3.2.2",
    "eslint-config-google": "^0.6.0",
    "eslint-plugin-react": "^6.0.0",
    "lodash": "~4.14.1",
    "react-addons-test-utils": "~15.3.0",
    "react": "~15.3.0",
    "react-dom": "~15.3.0"
  },
  "peerDependencies": {
    "classnames": "^2.*.*",
    "lodash": "^4.*.*",
    "react": "~15.3.*",
    "react-dom": "~15.3.*"
  }
}
